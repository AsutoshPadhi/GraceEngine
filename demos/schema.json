{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "program"
    ],
    "properties": {
      "program": {
        "$id": "#/properties/program",
        "type": "array",
        "title": "The Program Schema",
        "items": {
          "$id": "#/properties/program/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "card_id",
            "card_number",
            "card_type",
            "card_color",
            "display",
            "internal_dependants",
            "external_dependant",
            "children"
          ],
          "properties": {
            "card_id": {
              "$id": "#/properties/program/items/properties/card_id",
              "type": "string",
              "title": "The Card_id Schema",
              "default": "",
              "examples": [
                "variable_setter"
              ],
              "pattern": "^(.*)$"
            },
            "card_number": {
              "$id": "#/properties/program/items/properties/card_number",
              "type": "integer",
              "title": "The Card_number Schema",
              "default": 0,
              "examples": [
                1
              ]
            },
            "card_type": {
              "$id": "#/properties/program/items/properties/card_type",
              "type": "array",
              "title": "The Card_type Schema",
              "items": {
                "$id": "#/properties/program/items/properties/card_type/items",
                "type": "boolean",
                "title": "The Items Schema",
                "default": false,
                "examples": [
                  true,
                  true,
                  false,
                  false,
                  false,
                  true,
                  0
                ]
              }
            },
            "card_color": {
              "$id": "#/properties/program/items/properties/card_color",
              "type": "string",
              "title": "The Card_color Schema",
              "default": "",
              "examples": [
                "color_normal"
              ],
              "pattern": "^(.*)$"
            },
            "display": {
              "$id": "#/properties/program/items/properties/display",
              "type": "array",
              "title": "The Display Schema",
              "items": {
                "$id": "#/properties/program/items/properties/display/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                  "val_type",
                  "text"
                ],
                "properties": {
                  "val_type": {
                    "$id": "#/properties/program/items/properties/display/items/properties/val_type",
                    "type": "string",
                    "title": "The Val_type Schema",
                    "default": "",
                    "examples": [
                      "text"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "text": {
                    "$id": "#/properties/program/items/properties/display/items/properties/text",
                    "type": "string",
                    "title": "The Text Schema",
                    "default": "",
                    "examples": [
                      "set"
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              }
            },
            "internal_dependants": {
              "$id": "#/properties/program/items/properties/internal_dependants",
              "type": "array",
              "title": "The Internal_dependants Schema"
            },
            "external_dependant": {
              "$id": "#/properties/program/items/properties/external_dependant",
              "type": "object",
              "title": "The External_dependant Schema",
              "required": [
                "card_id",
                "card_number",
                "card_type",
                "card_color",
                "display"
              ],
              "properties": {
                "card_id": {
                  "$id": "#/properties/program/items/properties/external_dependant/properties/card_id",
                  "type": "string",
                  "title": "The Card_id Schema",
                  "default": "",
                  "examples": [
                    "expression"
                  ],
                  "pattern": "^(.*)$"
                },
                "card_number": {
                  "$id": "#/properties/program/items/properties/external_dependant/properties/card_number",
                  "type": "integer",
                  "title": "The Card_number Schema",
                  "default": 0,
                  "examples": [
                    2
                  ]
                },
                "card_type": {
                  "$id": "#/properties/program/items/properties/external_dependant/properties/card_type",
                  "type": "array",
                  "title": "The Card_type Schema",
                  "items": {
                    "$id": "#/properties/program/items/properties/external_dependant/properties/card_type/items",
                    "type": "boolean",
                    "title": "The Items Schema",
                    "default": false,
                    "examples": [
                      false,
                      false,
                      true,
                      false,
                      false,
                      false,
                      0
                    ]
                  }
                },
                "card_color": {
                  "$id": "#/properties/program/items/properties/external_dependant/properties/card_color",
                  "type": "string",
                  "title": "The Card_color Schema",
                  "default": "",
                  "examples": [
                    "color_logic"
                  ],
                  "pattern": "^(.*)$"
                },
                "display": {
                  "$id": "#/properties/program/items/properties/external_dependant/properties/display",
                  "type": "array",
                  "title": "The Display Schema",
                  "items": {
                    "$id": "#/properties/program/items/properties/external_dependant/properties/display/items",
                    "type": "object",
                    "title": "The Items Schema",
                    "required": [
                      "val_type",
                      "sticker_name",
                      "sticker_value"
                    ],
                    "properties": {
                      "val_type": {
                        "$id": "#/properties/program/items/properties/external_dependant/properties/display/items/properties/val_type",
                        "type": "string",
                        "title": "The Val_type Schema",
                        "default": "",
                        "examples": [
                          "sticker"
                        ],
                        "pattern": "^(.*)$"
                      },
                      "sticker_name": {
                        "$id": "#/properties/program/items/properties/external_dependant/properties/display/items/properties/sticker_name",
                        "type": "string",
                        "title": "The Sticker_name Schema",
                        "default": "",
                        "examples": [
                          "number"
                        ],
                        "pattern": "^(.*)$"
                      },
                      "sticker_value": {
                        "$id": "#/properties/program/items/properties/external_dependant/properties/display/items/properties/sticker_value",
                        "type": "integer",
                        "title": "The Sticker_value Schema",
                        "default": 0,
                        "examples": [
                          1
                        ]
                      }
                    }
                  }
                }
              }
            },
            "children": {
              "$id": "#/properties/program/items/properties/children",
              "type": "array",
              "title": "The Children Schema"
            }
          }
        }
      }
    }
  }
  